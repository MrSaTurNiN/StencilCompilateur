//test all
int main() {
	int i;
	int b = 5;
    int a = b + 4;
    printf("a = ");
    printi(a);
    printf("\n");
	for (i = 1; i < 6; i++) {
		if (i > 3) {
            b = b + i;
            printf("b = ");
            printi(b);
            printf("\n");
        }
        else {  //commentaire
            int c = -b;
            while (c <= 5) {
                b = b + b * c;
                c--;
                printf("b = ");
                printi(b);
                printf("\n");
            }
        }
	}
    printf("b = ");
    printi(b);
    printf("\n");
	return 0;
}
